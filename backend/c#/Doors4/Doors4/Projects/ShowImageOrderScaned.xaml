<Window x:Class="Doors4.Projects.ShowImageOrderScaned"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Doors4.Projects"
        mc:Ignorable="d"
        WindowState="Maximized"
        Title="ShowImageOrderScaned" Loaded="Window_Loaded" Closing="Window_Closing">
    <Grid>
        <Image x:Name="imageorder"  Margin="10,10,10,10" HorizontalAlignment="Center" VerticalAlignment="Center"/>
        <Button Content="Delete and  close" x:Name="btndeleteimge" Margin="0,0,10,10" Width="100" Click="btndeleteimge_Click" Height="50" VerticalAlignment="Bottom" HorizontalAlignment="Right" />
        <Button Content="save and  close" Visibility="Visible" x:Name="btnsaveimge" Margin="0,0,120,10" Height="50" Width="100"  VerticalAlignment="Bottom" HorizontalAlignment="Right" Click="btnsaveimge_Click" />

    </Grid>
</Window>
